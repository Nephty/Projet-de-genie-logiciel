                    <InnerShadow height="5.0" offsetX="-2.5" offsetY="-2.5" radius="2.0" width="5.0"/>
                </effect>
            </ComboBox>
            <Label fx:id="invalidSWIFTLabel" layoutX="187.0" layoutY="80.0" prefHeight="16.0" prefWidth="140.0"
                   text="%SignUpScene.invalidSWIFTLabel" textFill="#c80000" visible="false"/>
            <Label fx:id="invalidCityLabel" layoutX="187.0" layoutY="170.0" prefHeight="16.0" prefWidth="140.0"
                   text="%SignUpScene.invalidCityLabel" textFill="#c80000" visible="false"/>
